<typesystem package="pylabplot">
    <load-typesystem name="typesystem_widgets.xml" generate="no"/>
    <!-- src/backend/core -->
    <enum-type name="AspectType"/>
    <object-type name="AbstractAspect">
        <enum-type name="ChildIndexFlag" flags="ChildIndexFlags"/>
        <enum-type name="NameHandling"/>
        <modify-function signature="AbstractAspect(QString, AspectType)" remove="all"/>
        <modify-field name="m_type" remove="all"/>
        <modify-function signature="addChild(AbstractAspect*, QUndoCommand*)">
            <inject-code class="target" position="end">
                Py_INCREF(%PYARG_1);
            </inject-code>
        </modify-function>
    </object-type>
    <object-type name="AbstractColumn">
        <enum-type name="PlotDesignation"/>
        <enum-type name="TimeUnit"/>
        <enum-type name="ColumnMode"/>
        <enum-type name="Properties"/>
        <enum-type name="Formatting"/>
        <modify-function signature="AbstractColumn(QString, AspectType)" remove="all"/>
    </object-type>
    <object-type name="AbstractFilter">
        <modify-function signature="AbstractFilter(QString)" remove="all"/>
    </object-type>
    <object-type name="AbstractSimpleFilter">
        <modify-function signature="AbstractSimpleFilter()" remove="all"/>
    </object-type>
    <object-type name="SimpleFilterColumn"/>
    <object-type name="AbstractPart">
        <modify-function signature="AbstractPart(QString, AspectType)" remove="all"/>
    </object-type>

    <!-- src/backend/core/column -->
    <object-type name="Column">
        <object-type name="FormulaData"/>
        <modify-function signature="indexForValue(double, QVector&lt;double&gt;&amp;, AbstractColumn::Properties)" remove="all"/>
        <modify-function signature="indexForValue(double, QVector&lt;QPointF&gt;, AbstractColumn::Properties)" remove="all"/>
        <modify-function signature="indexForValue(double, QVector&lt;QLineF&gt;&amp;, AbstractColumn::Properties)" remove="all"/>
    </object-type>

    <!-- src/backend/datasources -->
    <object-type name="AbstractDataSource">
        <modify-function signature="AbstractDataSource(QString, AspectType)" remove="all"/>
    </object-type>

    <!-- src/backend/datasources/filters -->
    <object-type name="AbstractFileFilter">
        <enum-type name="FileType"/>
        <enum-type name="ImportMode"/>
        <modify-function signature="AbstractFileFilter(AbstractFileFilter::FileType)" remove="all"/>
        <modify-field name="m_type" remove="all"/>
    </object-type>
    <object-type name="AsciiFilter"/>
    <object-type name="BinaryFilter">
        <enum-type name="DataType"/>
    </object-type>
    <object-type name="CANFilter">
        <enum-type name="TimeHandling"/>
    </object-type>
    <object-type name="FITSFilter">
        <object-type name="Keyword"/>
        <object-type name="KeywordUpdate"/>
    </object-type>
    <object-type name="HDF5Filter"/>
    <object-type name="ImageFilter">
        <enum-type name="ImportFormat"/>
    </object-type>
    <object-type name="JsonFilter">
        <enum-type name="DataContainerType"/>
    </object-type>
    <object-type name="MatioFilter"/>
    <object-type name="NetCDFFilter"/>
    <object-type name="OdsFilter"/>
    <object-type name="ReadStatFilter"/>
    <object-type name="ROOTFilter">
        <object-type name="Directory"/>
    </object-type>
    <object-type name="SpiceFilter"/>
    <object-type name="VectorBLFFilter"/>
    <object-type name="XLSXFilter">
        <modify-function signature="sheets(QString, bool*)" remove="all"/>
    </object-type>

    <!-- src/backend/lib -->
    <object-type name="XmlStreamReader"/>

    <!-- src/backend/spreadsheet -->
    <object-type name="Spreadsheet">
        <object-type name="Linking"/>
    </object-type>
    <object-type name="StatisticsSpreadsheet">
        <enum-type name="Metric" flags="Metrics"/>
    </object-type>

    <!-- src/backend/worksheet -->
    <object-type name="Background">
        <enum-type name="Position"/>
        <enum-type name="Type"/>
        <enum-type name="ColorStyle"/>
        <enum-type name="ImageStyle"/>
    </object-type>
    <object-type name="Image"/>
    <object-type name="InfoElement">
        <object-type name="MarkerPoints_T"/>
    </object-type>
    <object-type name="Line"/>
    <object-type name="ResizeItem"/>
    <object-type name="TextLabel">
        <enum-type name="Type"/>
        <enum-type name="Mode"/>
        <enum-type name="BorderShape"/>
        <object-type name="TextWrapper"/>
        <object-type name="GluePoint"/>
    </object-type>
    <object-type name="Worksheet">
        <enum-type name="Unit"/>
        <enum-type name="Layout"/>
        <enum-type name="CartesianPlotActionMode"/>
        <enum-type name="ZoomFit"/>
    </object-type>
    <object-type name="WorksheetElement">
        <enum-type name="Orientation"/>
        <enum-type name="HorizontalPosition"/>
        <enum-type name="VerticalPosition"/>
        <enum-type name="HorizontalAlignment"/>
        <enum-type name="VerticalAlignment"/>
        <enum-type name="PositionLimit"/>
        <modify-function signature="WorksheetElement(QString, AspectType)" remove="all"/>
        <modify-field name="cSystem" remove="all"/>
    </object-type>
    <object-type name="WorksheetElementContainer">
        <modify-function signature="WorksheetElementContainer(QString, AspectType)" remove="all"/>
    </object-type>
    <object-type name="WorksheetElementGroup">
        <modify-function signature="WorksheetElementGroup(QString)" remove="all"/>
    </object-type>

    <!-- src/backend/worksheet/plots -->
    <object-type name="AbstractCoordinateSystem">
        <enum-type name="MappingFlag" flags="MappingFlags"/>
        <!-- <object-type name="LineClipResult"/> -->
        <modify-function signature="AbstractCoordinateSystem(AbstractPlot*)" remove="all"/>
    </object-type>
    <object-type name="AbstractPlot">
        <modify-function signature="AbstractPlot(QString, AspectType)" remove="all"/>
    </object-type>
    <object-type name="PlotArea">
        <enum-type name="BorderTypeFlags" flags="BorderType"/>
    </object-type>

    <!-- src/backend/worksheet/plots/cartesian -->
    <object-type name="Axis">
        <enum-type name="RangeType"/>
        <enum-type name="Position"/>
        <enum-type name="LabelsFormat"/>
        <enum-type name="TicksFlags" flags="TicksDirection"/>
        <enum-type name="TicksType"/>
        <enum-type name="ArrowType"/>
        <enum-type name="ArrowPosition"/>
        <enum-type name="LabelsPosition"/>
        <enum-type name="LabelsTextType"/>
        <enum-type name="LabelsBackgroundType"/>
        <enum-type name="TicksStartType"/>
    </object-type>
    <object-type name="BarPlot">
        <enum-type name="Type"/>
    </object-type>
    <object-type name="BoxPlot">
        <enum-type name="Ordering"/>
        <enum-type name="WhiskersType"/>
    </object-type>
    <object-type name="CartesianCoordinateSystem">
        <enum-type name="Dimension"/>
    </object-type>
    <object-type name="CartesianPlot">
        <enum-type name="Type"/>
        <enum-type name="MouseMode"/>
        <enum-type name="NavigationOperation"/>
        <enum-type name="RangeType"/>
        <enum-type name="RangeBreakStyle"/>
        <object-type name="RangeBreak"/>
        <object-type name="RangeBreaks"/>
    </object-type>
    <object-type name="CartesianPlotLegend"/>
    <object-type name="CartesianScale">
        <!-- <modify-function signature="CartesianScale()" remove="all"/> -->
    </object-type>
    <object-type name="CustomPoint"/>
    <object-type name="ErrorBar">
        <enum-type name="ErrorType"/>
        <enum-type name="Type"/>
        <enum-type name="Dimension"/>
    </object-type>
    <object-type name="Histogram">
        <enum-type name="Type"/>
        <enum-type name="Normalization"/>
        <enum-type name="BinningMethod"/>
        <enum-type name="LineType"/>
        <enum-type name="ValuesType"/>
        <enum-type name="ValuesPosition"/>
    </object-type>
    <object-type name="KDEPlot"/>
    <object-type name="LollipopPlot"/>
    <object-type name="Plot">
        <!-- <modify-function signature="Plot()" remove="all"/> -->
    </object-type>
    <object-type name="QQPlot"/>
    <object-type name="ReferenceLine"/>
    <object-type name="ReferenceRange"/>
    <object-type name="Symbol">
        <enum-type name="Style"/>
    </object-type>
    <object-type name="Value">
        <enum-type name="Type"/>
        <enum-type name="Position"/>
    </object-type>
    <object-type name="XYAnalysisCurve">
        <!-- <modify-function signature="XYAnalysisCurve()" remove="all"/> -->
        <enum-type name="DataSourceType"/>
        <enum-type name="AnalysisAction"/>
        <value-type name="Result"/>
    </object-type>
    <object-type name="XYConvolutionCurve">
        <object-type name="ConvolutionData"/>
    </object-type>
    <object-type name="XYCorrelationCurve">
        <object-type name="CorrelationData"/>
    </object-type>
    <object-type name="XYCurve">
        <enum-type name="LineType"/>
        <enum-type name="DropLineType"/>
        <enum-type name="ValuesType"/>
        <enum-type name="ValuesPosition"/>
    </object-type>
    <object-type name="XYDataReductionCurve">
        <object-type name="DataReductionData"/>
        <object-type name="DataReductionResult"/>
    </object-type>
    <object-type name="XYDifferentiationCurve">
        <object-type name="DifferentiationData"/>
    </object-type>
    <object-type name="XYEquationCurve">
        <object-type name="EquationData"/>
        <enum-type name="EquationType"/>
    </object-type>
    <object-type name="XYFitCurve">
        <object-type name="FitData"/>
        <object-type name="FitResult"/>
        <modify-function signature="initFitData(XYFitCurve::FitData&amp;)" remove="all"/>
    </object-type>
    <object-type name="XYFourierFilterCurve">
        <object-type name="FilterData"/>
        <object-type name="FilterResult"/>
    </object-type>
    <object-type name="XYFourierTransformCurve">
        <object-type name="TransformData"/>
    </object-type>
    <object-type name="XYHilbertTransformCurve">
        <object-type name="TransformData"/>
    </object-type>
    <object-type name="XYIntegrationCurve">
        <object-type name="IntegrationData"/>
        <object-type name="IntegrationResult"/>
    </object-type>
    <object-type name="XYInterpolationCurve">
        <enum-type name="PointsMode"/>
        <object-type name="InterpolationData"/>
    </object-type>
    <object-type name="XYSmoothCurve">
        <object-type name="SmoothData"/>
    </object-type>

    <object-type name="Folder"/> <!-- inherits AbstractAspect, QObject -->
    <object-type name="Project"> <!-- inherits Folder, AbstractAspect, QObject -->
        <modify-function signature="Project()" remove="all"/>
        <modify-function signature="project()" remove="all"/>
    </object-type>
    <object-type name="Workbook"/> <!-- inherits AbstractPart, AbstractAspect, QObject -->

    <!-- src/backend/matrix -->
    <object-type name="Matrix"/> <!-- inherits AbstractDataSource, AbstractPart, AbstractAspect, QObject -->
    
    <!-- src/backend/script -->
    <object-type name="Script"/> <!-- inherits AbstractPart, AbstractAspect, QObject -->
    <object-type name="PythonLogger">
        <!-- <modify-function signature="PythonLogger(PythonScript*, bool)" remove="all"/> -->
    </object-type>

    <!-- src/backend/note -->
    <object-type name="Note"/> <!-- inherits AbstractPart, AbstractAspect, QObject -->
    
    <!-- src/backend/datapicker -->
    <object-type name="Datapicker"/> <!-- inherits AbstractPart, AbstractAspect, QObject -->
</typesystem>